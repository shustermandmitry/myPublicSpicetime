{
  "name": "@treenity/accounts-mod",
  "version": "0.0.94",
  "description": "Treenity accounts",
  "author": "Treenity",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "devwatch": "tsx watch src/server.ts",
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "typecheck": "tsc",
    "test": "jest --passWithNoTests",
    "clean": "treenity-clean",
    "extract-schemas": "extract-entity-schemas"
  },
  "types": "./dist/browser/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/browser/index.d.ts",
      "node": "./dist/node/index.mjs",
      "default": "./dist/browser/index.mjs"
    },
    "./client": {
      "types": "./dist/browser/client.d.ts",
      "default": "./dist/browser/client.mjs"
    },
    "./mods.types": {
      "types": "./dist/browser/mods.types.d.ts",
      "node": "./dist/node/mods.types.mjs",
      "default": "./dist/browser/mods.types.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/browser/index.d.ts"
      ],
      "mods.types": [
        "./dist/browser/mods.types.d.ts"
      ],
      "client": [
        "./dist/browser/client.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@treenity/core": "1.0.44",
    "@treenity/entity": "0.5.37",
    "@treenity/feathers-service": "0.0.39",
    "@treenity/js-shared": "1.0.33",
    "@treenity/repository": "0.3.27",
    "@treenity/tree-api": "0.1.50",
    "@treenity/ui-kit": "2.1.72",
    "fast-json-patch": "3.1.1",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@treenity/build-utils": "1.1.30",
    "@treenity/tsconfig": "1.0.13",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "rollup": "^4.26.0",
    "typescript": "^5.4.5"
  }
}
