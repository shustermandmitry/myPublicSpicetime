{
  "name": "@treenity/core",
  "version": "1.0.44",
  "description": "treenity core",
  "author": "Treenity",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "clean": "treenity-clean",
    "typecheck": "tsc",
    "test": "jest --no-cache  --detectOpenHandles --runInBand --forceExit",
    "--doc": "typedoc --plugin typedoc-plugin-markdown"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "@treenity/call-chain": "1.0.29",
    "@treenity/js-shared": "1.0.33",
    "json-schema": "^0.4.0",
    "json-schema-to-ts": "^3.1.0",
    "synchronous-promise": "2.0.17",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@treenity/build-utils": "1.1.30",
    "@treenity/tsconfig": "1.0.13",
    "@types/jest": "^29.5.12",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.7.5",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.3.12",
    "ajv": "^8.12.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "rollup": "^4.26.0",
    "typescript": "^5.4.5"
  }
}
