{
  "name": "@treenity/repository",
  "version": "0.3.27",
  "description": "Treenity Repository",
  "author": "Treenity",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "dev": "tsx src/server.ts",
    "devwatch": "tsx watch src/server.ts",
    "typecheck": "tsc",
    "test": "cross-env NODE_ENV=test jest",
    "clean": "treenity-clean",
    "extract-schemas": "extract-entity-schemas"
  },
  "types": "./dist/browser/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/browser/index.d.ts",
      "node": "./dist/node/index.mjs",
      "default": "./dist/browser/index.mjs"
    },
    "./mods": {
      "types": "./dist/browser/mods.d.ts",
      "node": "./dist/node/mods.mjs",
      "default": "./dist/browser/mods.mjs"
    },
    "./client": {
      "types": "./dist/browser/client.d.ts",
      "node": "./dist/node/client.mjs",
      "default": "./dist/browser/client.mjs"
    },
    "./mods.types": {
      "types": "./dist/browser/mods.types.d.ts",
      "node": "./dist/node/mods.types.mjs",
      "default": "./dist/browser/mods.types.mjs"
    },
    "./server.utils": {
      "types": "./dist/browser/server.utils.d.ts",
      "node": "./dist/node/server.utils.mjs",
      "default": "./dist/browser/server.utils.mjs"
    },
    "./entity": {
      "types": "./dist/browser/entity.d.ts",
      "node": "./dist/node/entity.mjs",
      "default": "./dist/browser/entity.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/browser/index.d.ts"
      ],
      "client": [
        "./dist/browser/client.d.ts"
      ],
      "mods": [
        "./dist/browser/mods.d.ts"
      ],
      "mods.types": [
        "./dist/browser/mods.types.d.ts"
      ],
      "server.utils": [
        "./dist/browser/server.utils.d.ts"
      ],
      "entity": [
        "./dist/browser/entity.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "5.0.25",
    "@feathersjs/authentication": "5.0.25",
    "@feathersjs/authentication-client": "5.0.25",
    "@feathersjs/authentication-local": "5.0.25",
    "@feathersjs/commons": "5.0.25",
    "@feathersjs/configuration": "5.0.25",
    "@feathersjs/errors": "5.0.25",
    "@feathersjs/express": "5.0.25",
    "@feathersjs/feathers": "5.0.25",
    "@feathersjs/hooks": "^0.9.0",
    "@feathersjs/schema": "5.0.25",
    "@feathersjs/socketio": "5.0.25",
    "@feathersjs/socketio-client": "5.0.25",
    "@feathersjs/transport-commons": "5.0.25",
    "@remote-ui/rpc": "^1.4.5",
    "@s-libs/micro-dash": "^17.1.0",
    "@snickbit/feathers-redis": "^1.0.20",
    "@telegram-apps/init-data-node": "^1.2.0",
    "@telegram-auth/server": "^1.0.4",
    "@treenity/core": "1.0.44",
    "@treenity/entity": "0.5.37",
    "@treenity/feathers-psql": "0.0.43",
    "@treenity/feathers-service": "0.0.39",
    "@treenity/js-shared": "1.0.33",
    "@treenity/json-schema": "0.1.37",
    "@treenity/sequelizer": "1.0.6",
    "@treenity/tree-api": "0.1.50",
    "@treenity/ui-kit": "2.1.72",
    "assert": "^2.1.0",
    "async-mutex": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "casbin": "^5.28.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.0",
    "fast-json-patch": "3.1.1",
    "feathers-sequelize": "^7.0.3",
    "form-data": "^4.0.0",
    "json-schema-to-ts": "^3.1.0",
    "mobx": "^6.13.5",
    "multer": "1.4.5-lts.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "sequelize": "6.37.3",
    "sift": "^17.0.1",
    "socket.io-client": "^4.7.4",
    "telegraf": "^4.15.3",
    "tslib": "^2.8.1",
    "uuid": "^10.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.25",
    "@feathersjs/memory": "^5.0.25",
    "@treenity/build-utils": "1.1.30",
    "@treenity/tsconfig": "1.0.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/json-schema": "^7.0.15",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.12",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.3.3",
    "rollup": "^4.26.0",
    "shx": "^0.3.4",
    "ts-jest-mock-import-meta": "^1.2.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  }
}
