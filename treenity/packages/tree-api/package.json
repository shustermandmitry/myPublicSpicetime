{
  "name": "@treenity/tree-api",
  "version": "0.1.50",
  "description": "tree api",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "clean": "treenity-clean",
    "test": "jest --no-cache  --detectOpenHandles --runInBand --forceExit",
    "doc": "typedoc --plugin typedoc-plugin-markdown",
    "extract": "extract-entity-schemas"
  },
  "type": "module",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "node": "./dist/node/index.mjs",
      "default": "./dist/browser/index.mjs"
    },
    "./server": {
      "node": "./dist/node/server.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/node/index.d.ts"
      ],
      "./server": [
        "./dist/node/server.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/txt-dev/cluster.git"
  },
  "author": "txt",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/txt-dev/ui/issues"
  },
  "homepage": "https://gitlab.com/txt-dev/ui#readme",
  "dependencies": {
    "@feathersjs/feathers": "5.0.25",
    "@s-libs/micro-dash": "^17.1.0",
    "@treenity/call-chain": "1.0.29",
    "@treenity/core": "1.0.44",
    "@treenity/entity": "0.5.37",
    "@treenity/feathers-service": "0.0.39",
    "@treenity/js-shared": "1.0.33",
    "@treenity/json-schema": "0.1.37",
    "fast-json-patch": "3.1.1",
    "immer": "10.0.3",
    "isomorphic-ws": "^5.0.0",
    "lodash": "npm:@s-libs/micro-dash@^18.0.0",
    "mobx": "^6.13.5",
    "react": "^18.3.1",
    "swr": "^2.3.0",
    "tslib": "^2.8.1",
    "uuid": "^10.0.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@treenity/build-utils": "1.1.30",
    "@treenity/tsconfig": "1.0.13",
    "@types/express": "4.17.21",
    "@types/express-ws": "^3.0.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "npm:@s-libs/micro-dash@^18.0.0",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.12",
    "@types/uuid": "^10.0.0",
    "@types/ws": "8.5.12",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "express": "^4.21.0",
    "express-ws": "^5.0.2",
    "jest": "^29.7.0",
    "rollup": "^4.26.0",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^5.4.5"
  }
}
