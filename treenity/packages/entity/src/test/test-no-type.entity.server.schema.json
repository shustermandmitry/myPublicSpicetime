{
    "$id": "@treenity/entity:server:no-type-var-entity",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Test server entity extended from the test client entity",
    "type": "object",
    "properties": {
        "serverOnlyProperty": {
            "description": "This is a server only property",
            "type": "string"
        },
        "a": {
            "type": "number",
            "default": 0
        },
        "b": {
            "type": "string",
            "default": "def"
        },
        "c": {
            "type": "string",
            "format": "date-time"
        },
        "d": {
            "type": "boolean"
        },
        "e": {
            "type": "string"
        }
    },
    "required": [
        "a",
        "b",
        "c",
        "serverOnlyProperty"
    ],
    "methods": {
        "callMe": {
            "description": "This should be called, if you want to call something",
            "arguments": [
                {
                    "name": "param",
                    "description": "The parameter to call with",
                    "type": "string"
                }
            ],
            "return": {
                "type": "string"
            }
        },
        "callWriteMe": {
            "arguments": [
                {
                    "name": "param",
                    "type": "string"
                }
            ],
            "return": {
                "type": "undefined"
            }
        },
        "callObj": {
            "arguments": [
                {
                    "name": "param",
                    "type": "object",
                    "properties": {
                        "a": {
                            "type": "string"
                        },
                        "b": {
                            "type": "number"
                        },
                        "c": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    "required": [
                        "a",
                        "b",
                        "c"
                    ]
                }
            ],
            "return": {
                "type": "string",
                "format": "date-time"
            }
        }
    }
}

