{
  "name": "@treenity/entity",
  "version": "0.5.37",
  "description": "treenity entity component system",
  "author": "Treenity",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "clean": "treenity-clean",
    "typecheck": "tsc",
    "extract-schemas": "src/bin/extract-entity-schemas.ts",
    "test": "jest --no-cache  --detectOpenHandles --runInBand --forceExit",
    "--doc": "typedoc --plugin typedoc-plugin-markdown"
  },
  "bin": {
    "extract-entity-schemas": "src/bin/extract-entity-schemas.ts"
  },
  "types": "./dist/browser/index.d.ts",
  "module": "./dist/browser/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/browser/index.d.ts",
      "node": "./dist/node/index.mjs",
      "default": "./dist/browser/index.mjs"
    },
    "./react": {
      "node": "./dist/node/react.mjs",
      "default": "./dist/browser/react.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/browser/index.d.ts"
      ],
      "./react": [
        "./dist/browser/react.d.ts"
      ]
    }
  },
  "dependencies": {
    "@feathersjs/hooks": "^0.9.0",
    "@s-libs/micro-dash": "^17.1.0",
    "@treenity/call-chain": "1.0.29",
    "@treenity/core": "1.0.44",
    "@treenity/js-shared": "1.0.33",
    "@treenity/json-schema": "0.1.37",
    "fast-json-patch": "3.1.1",
    "mobx": "^6.13.5",
    "mobx-utils": "^6.0.8",
    "tslib": "^2.8.1",
    "typescript-json-schema": "^0.64.0",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "mobx": "^6.13.5",
    "mobx-utils": "^6.0.8",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@treenity/build-utils": "1.1.30",
    "@treenity/tsconfig": "1.0.13",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.12",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "rollup": "^4.26.0"
  }
}
